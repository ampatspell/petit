<Block::Editor::Border @node={{@node}}>

  <Konva::Stage @size={{this.size}} as |parent|>
    <Konva::Node::Layer @parent={{parent}} as |parent|>
      <Block::Project::Editor::Node::Sequence::Content
        @parent={{parent}}
        @size={{this.size}}
        @frames={{this.frames}}
      />
    </Konva::Node::Layer>
  </Konva::Stage>

</Block::Editor::Border>
