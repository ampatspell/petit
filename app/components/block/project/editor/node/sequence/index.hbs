<Block::Editor::Border @node={{@node}}>

  <Konva::Stage @size={{this.size}} as |parent|>
    <Konva::Node::Layer @parent={{parent}} as |parent|>
      <Block::Konva::Frames
        @parent={{parent}}
        @size={{this.size}}
        @frames={{this.frames}}
        @framerate={{this.framerate}}
        @key="frame"
      />
    </Konva::Node::Layer>
  </Konva::Stage>

</Block::Editor::Border>
