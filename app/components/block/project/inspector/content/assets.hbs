{{#if this.sprites}}
  <Block::Project::Inspector::Content::Assets::Sprites
    @content={{@content}}
    @model={{@model}}
    @sprite={{@sprite}}
    @frame={{@frame}}
    @onSelect={{@onSelect}}
  />
{{/if}}

{{#if this.sequences}}
  <Block::Project::Inspector::Content::Assets::Sequences
    @content={{@content}}
    @model={{@model}}
    @sequence={{@sequence}}
    @onSelect={{@onSelect}}
  />
{{/if}}