<div class="group frames">
  <Konva::Stage class="stage" @size={{this.size}} as |parent|>
    <Konva::Node::Layer @parent={{parent}} as |parent|>
      <Block::Konva::Frames
        @parent={{parent}}
        @size={{this.size}}
        @frames={{this.frames}}
        @key={{@key}}
      />
    </Konva::Node::Layer>
  </Konva::Stage>
</div>