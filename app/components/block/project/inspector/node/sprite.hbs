<@content.tabs @tabs={{this.tabs}} @tab={{this.tab}} @label="label" @onSelect={{this.selectTab}}/>

{{#if (eq this.tab.id 'sprite')}}

  <Block::Project::Inspector::Node::Sprite::Content @model={{@model}} @content={{@content}}/>

{{else if (eq this.tab.id 'colors')}}

  <Block::Project::Inspector::Node::Sprite::Colors @model={{@model}} @content={{@content}}/>

{{/if}}
