<@content.header @name={{@model.identifier}}/>
<@content.input @label="Identifier" @key="identifier"/>
<@content.actions as |actions|>
  <actions.button @label="Add new layer" @onClick={{this.addLayer}}/>
</@content.actions>
<@content.reorder/>
<@content.delete @label="Delete scene" @confirmation="Are you sure you want to delete this scene?"/>