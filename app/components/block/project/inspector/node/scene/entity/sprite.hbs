<Block::Project::Inspector::Node::Scene::Entity::Base @content={{@content}} @model={{@model}}>
  <:content>
    <@content.reference @label="Sprite" @collection="sprite" @key="sprite"/>
    {{#if @model.sprite.model}}
      <@content.reference @label="Frame" @models={{@model.sprite.model.identifiedFrames}} @key="frame"/>
    {{/if}}
  </:content>
  <:assets>
    <@content.assets/>
  </:assets>
</Block::Project::Inspector::Node::Scene::Entity::Base>