<div class="route-projects-project-index">

  <Block::Project::Nodes::Tree @nodes={{@project.nodes}} class="tree"/>

  <div class="content">
    <div class="title">{{@project.title}} ({{dt @project.createdAt}})</div>

    <div class="actions">
      <Input::Button @value="Delete" @onClick={{this.delete}}/>
    </div>
  </div>

</div>
